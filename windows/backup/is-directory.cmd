@ECHO OFF

SET FILESYSTEM_OBJECT=%~1

IF NOT "%FILESYSTEM_OBJECT%"=="" (
	IF EXIST "%FILESYSTEM_OBJECT%\" (
		SET RETURN_CODE=1
		GOTO EXIT
	) ELSE (
		IF EXIST "%FILESYSTEM_OBJECT%" (
			SET RETURN_CODE=2
		GOTO EXIT
		) ELSE (
			SET RETURN_CODE=3
		GOTO EXIT
		)
	)
) ELSE (
	SET RETURN_CODE=3
		GOTO EXIT
)

:EXIT

EXIT /B %RETURN_CODE%